<!-- Limit events based on origin time, magnitude, depth, and location -->
<eventArm>
    <fdsnEvent> <!-- default use USGS catalog -->
        <originTimeRange>
            <startTime>2010-01-01T00:00:00.000Z</startTime>
            <endTime>2011-01-01T00:00:00.000Z</endTime>
        </originTimeRange>
        <magnitudeRange>
            <min>5.8</min>
            <max>8.0</max>
        </magnitudeRange>
        <originDepthRange>
            <unit>KILOMETER</unit>
            <min>30</min>
            <max>300</max>
        </originDepthRange>
        <boxArea>
            <latitudeRange>
                <min>33.46</min>
                <max>33.53</max>
            </latitudeRange>
            <longitudeRange>
                <min>-116.82</min>
                <max>-116.765</max>
            </longitudeRange>
        </boxArea>
    </fdsnEvent>
    <printlineEventProcess/>
    <CSVEventPrinter> <!-- save catalog to csv file -->
        <filename>events.csv</filename>
    </CSVEventPrinter>
</eventArm>
